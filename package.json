{
	"name": "portailetu",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "rm -r node_modules/.vite-temp; bunx --bun vite dev",
		"build": "rm -r node_modules/.vite-temp; bunx --bun vite build",
		"preview": "rm -r node_modules/.vite-temp; bunx --bun vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^5.2.7",
		"@sveltejs/adapter-auto": "^6.1.1",
		"@sveltejs/kit": "^2.46.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"bun-types": "^1.2.23",
		"svelte": "^5.39.11",
		"svelte-check": "^4.3.3",
		"typescript": "^5.9.3",
		"vite": "^7.1.9",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@auth/core": "^0.41.0",
		"@auth/sveltekit": "^1.11.0",
		"svelte-adapter-bun": "^1.0.0"
	},
	"overrides": {
		"cookie": "^0.7.0"
	}
