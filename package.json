{
	"name": "portailetu",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "rm -r node_modules/.vite-temp; vite dev",
		"build": "rm -r node_modules/.vite-temp; vite build",
		"preview": "rm -r node_modules/.vite-temp; vite preview",
		"prepare": "husky",
		"lint": "eslint --ext .js,.ts,.svelte .",
		"lint:fix": "eslint --ext .js,.ts,.svelte . --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"tsc-check": "tsc --noEmit"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^5.2.7",
		"@sveltejs/adapter-auto": "7.0.0",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"bun-types": "^1.3.2",
		"globals": "^16.5.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"svelte": "^5.43.12",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"vite": "^7.2.4",
		"vitest": "^4.0.10"
	},
	"dependencies": {
		"@auth/core": "^0.41.1",
		"@auth/sveltekit": "^1.11.1",
		"@typescript-eslint/eslint-plugin": "^8.47.0",
		"@typescript-eslint/parser": "^8.47.0",
		"dotenv": "^17.2.3",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"mysql2": "^3.15.3",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte-adapter-bun": "^1.0.1",
		"svelte-markdown": "^0.4.1"
	},
	"overrides": {
		"cookie": "^0.7.0"
	},
	"lint-staged": {
		"*.{js,ts,svelte}": [
			"prettier --write",
			"eslint --fix"
		],
		"*.{json,css,md}": [
			"prettier --write"
		]
	}
}
