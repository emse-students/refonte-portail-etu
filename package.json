{
	"name": "portailetu",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "rm -r node_modules/.vite-temp; bunx --bun vite dev",
		"build": "rm -r node_modules/.vite-temp; bunx --bun vite build",
		"preview": "rm -r node_modules/.vite-temp; bunx --bun vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^5.0.0",
		"@neoconfetti/svelte": "^2.0.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"svelte": "^5.25.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4",
		"bun-types": "^1.2.21"
	},
	"dependencies": {
		"jose": "6.1.0",
		"svelte-adapter-bun": "^0.5.2",
		"sveltekit-oidc": "^0.0.8"
	}
}
